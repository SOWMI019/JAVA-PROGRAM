import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner scanner=new Scanner(System.in);
		int n=scanner.nextInt();
                 int ans=0;
                for(int i=1;i<=n/2;i++)
{
if(n%i==0){ 
ans+=i;         
}
}
System.out.println(n+(ans==n?"its is a perfect number":"not a perfect number"));


}
}
