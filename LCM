import java.util.Scanner;
class Main{
public static void main(String[]args)
{
Scanner scan=new Scanner(System.in);
int n1=scan.nextInt();
int n2=scan.nextInt();
int a=n1,b=n2;

while(n2!=0){
int temp=n2;
n2=n1%n2;
n1=temp;

}
int gcd=n1;
int lcm=(a*b/gcd);
System.out.println("LCM of two numbers : "+lcm);
}
}

